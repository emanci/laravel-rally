{
    "name": "questocat/laravel-rally",
    "description": "A followers system for laravel 5",
    "keywords": ["follow", "like", "favorite", "remember", "subscribe"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [{
        "name": "questocat",
        "email": "zhengchaopu@gmail.com"
    }],
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "laravel/laravel": "^5.0",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Questocat\\Rally\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Questocat\\Rally\\RallyServiceProvider"
            ]
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
